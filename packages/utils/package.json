{
  "name": "@s1seven/schema-tools-utils",
  "description": "Utils used in @s1seven/schema-tools packages",
  "version": "0.1.2",
  "license": "Apache-2.0",
  "author": "getlarge <ed@getlarge.eu>",
  "homepage": "https://github.com/s1seven/schema-tools/tree/master/packages/utils#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/s1seven/schema-tools/.git"
  },
  "bugs": {
    "url": "https://github.com/s1seven/schema-tools/issues"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "publicConfig": {
    "access": "public"
  },
  "private": false,
  "main": "dist/index",
  "types": "dist/index",
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=12",
    "npm": ">=6"
  },
  "scripts": {
    "build": "npm run clean && npm run compile",
    "clean": "rimraf -rf ./dist",
    "cleanup": "rimraf -rf dist coverage",
    "compile": "tsc -p tsconfig.build.json",
    "lint": "eslint -c ../../.eslintrc.js \"{src,apps,libs,test}/**/*.ts\" --fix",
    "prepublishOnly": "npm run build",
    "test": "jest",
    "test:cov": "rimraf -rf ./coverage && jest --ci --coverage",
    "test:staged": "jest --runInBand --onlyChanged",
    "scan:deps": "audit-ci --config ../../audit-ci.json",
    "cast-cert-test": "node example.js"
  },
  "dependencies": {
    "@s1seven/schema-tools-types": "^0.4.0",
    "ajv": "^8.10.0",
    "axios": "^0.26.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "node-cache": "^5.1.2",
    "reflect-metadata": "^0.1.13",
    "semver-lite": "0.0.6"
  },
  "gitHead": "5526910fd5974d09e80b5ba6324043b4409dd179"
}
