{
  "name": "@s1seven/schema-tools-generate-en10168-pdf-template",
  "version": "0.6.6",
  "license": "Apache-2.0",
  "main": "dist/generateContent.js",
  "homepage": "https://github.com/s1seven/schema-tools/tree/master/packages/generate-en10168-pdf-template#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/s1seven/schema-tools/.git"
  },
  "bugs": {
    "url": "https://github.com/s1seven/schema-tools/issues"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "publicConfig": {
    "access": "public"
  },
  "files": [
    "dist"
  ],
  "targets": {
    "main": {
      "engines": {
        "node": ">=12.x"
      },
      "distDir": "./dist",
      "optimize": true,
      "outputFormat": "commonjs",
      "sourceMap": false,
      "includeNodeModules": {
        "@s1seven/schema-tools-generate-pdf-template-helpers": true,
        "@s1seven/schema-tools-generate-types": true
      }
    }
  },
  "scripts": {
    "build": "PARCEL_WORKER_BACKEND=process parcel build './src/generateContent.ts' --no-cache --target=main",
    "compile": "npm run create-interfaces && npm run build",
    "lint": "eslint -c ../../.eslintrc.js \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:cov": "rimraf -rf ./coverage && jest --ci --coverage",
    "test:staged": "jest --runInBand --onlyChanged",
    "scan:deps": "audit-ci --config ../../audit-ci.json",
    "create-example": "node --icu-data-dir=`echo $(./node_modules/.bin/node-full-icu-path)` ./utils/create-example.js",
    "create-example:no-icu": "node ./utils/create-example.js",
    "create-interfaces": "node ./utils/create-schema-interfaces.js https://schemas.s1seven.com/en10168-schemas/v0.2.0/schema.json ./src/types/schemaTypes.ts"
  },
  "dependencies": {
    "@s1seven/schema-tools-generate-pdf-template-helpers": "^0.4.6",
    "@s1seven/schema-tools-types": "^0.5.0",
    "pdfmake": "0.2.5"
  },
  "devDependencies": {
    "@s1seven/schema-tools-generate-interfaces": "^0.0.32",
    "@s1seven/schema-tools-utils": "^0.2.3",
    "@types/pdfmake": "^0.2.0",
    "full-icu": "^1.4.0",
    "lato-font": "^3.0.0",
    "parcel": "^2.3.1"
  },
  "gitHead": "5526910fd5974d09e80b5ba6324043b4409dd179"
}
