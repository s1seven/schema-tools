{
  "name": "@s1seven/schema-tools-generate-interfaces",
  "description": "Tool to generate TS interfaces from schema",
  "version": "0.0.24",
  "license": "Apache-2.0",
  "author": "getlarge <ed@getlarge.eu>",
  "homepage": "https://github.com/s1seven/schema-tools//tree/master/packages/generate-interfaces#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/s1seven/schema-tools/.git"
  },
  "bugs": {
    "url": "https://github.com/s1seven/schema-tools/issues"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "publicConfig": {
    "access": "public"
  },
  "private": false,
  "main": "dist/index",
  "types": "dist/index",
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=12",
    "npm": ">=6"
  },
  "scripts": {
    "build": "npm run clean && npm run compile",
    "clean": "rimraf -rf ./dist",
    "cleanup": "rimraf -rf dist coverage",
    "compile": "tsc -p tsconfig.build.json",
    "lint": "eslint -c ../../.eslintrc.js \"{src,apps,libs,test}/**/*.ts\" --fix",
    "prepublishOnly": "npm run build",
    "test": "jest",
    "test:cov": "rimraf -rf ./coverage && jest --ci --coverage",
    "test:staged": "jest --runInBand --onlyChanged",
    "scan:deps": "audit-ci --config ../../audit-ci.json"
  },
  "dependencies": {
    "@s1seven/schema-tools-types": "^0.1.7",
    "@s1seven/schema-tools-utils": "^0.0.24",
    "json-schema-to-typescript": "^10.1.5",
    "lodash.merge": "^4.6.2"
  },
  "devDependencies": {
    "@types/ajv": "^1.0.0",
    "@types/lodash.merge": "^4.6.6",
    "axios": "^0.26.0",
    "rimraf": "~3.0.2",
    "typescript": "^4.4.3"
  },
  "gitHead": "5526910fd5974d09e80b5ba6324043b4409dd179"
}
