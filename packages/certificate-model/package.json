  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "publicConfig": {
    "access": "public"
  },
  "private": false,
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=12",
    "npm": ">=6"
